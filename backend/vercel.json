{
  "version": 2,
  "builds": [
    {
      "src": "dist/api/server.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "dist/agents/**",
          "dist/constant/**",
          "dist/api/**",
          "dist/mcp/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/api/server.js"
    }
  ]
}
